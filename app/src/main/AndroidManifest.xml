<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Numplex"
        tools:targetApi="31">
        <activity
            android:name=".ClassificationsActivity"
            android:exported="false"
            android:label="@string/title_activity_classification"
            android:theme="@style/Theme.Numplex.NoActionBar" />
        <activity
            android:name=".NumplexActivity"
            android:exported="false"
            android:label="@string/title_activity_navi_drawer"
            android:theme="@style/Theme.Numplex.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>